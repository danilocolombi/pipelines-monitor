{
  "manifestVersion": 1,
  "id": "pipelines-monitor",
  "publisher": "danilocolombi",
  "version": "0.0.2",
  "name": "Pipelines Monitor",
  "description": "A Azure DevOps extension designed to deliver insightful analytics on pipeline executions within your Team Project.",
  "public": true,
  "categories": ["Azure Pipelines"],
  "tags": ["Pipelines", "Dashboards"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "logo.png"
  },
  "content": {
    "details": {
      "path": "documentation/overview.md"
    },
    "license": {
      "path": "documentation/license.md"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/danilocolombi/pipelines-monitor"
  },
  "files": [
    {
      "path": "static",
      "addressable": true
    },
    {
      "path": "documentation",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    }
  ]
}